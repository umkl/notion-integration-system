{
  "name": "notion-integration-system",
  "version": "1.0.0",
  "description": "Update Notion-page-properties automatically and Integrate Notion-database-entrys with Google Calendar or Microsoft TODO .",
  "main": "src/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc -p .",
    "package": "pkg . --targets node14-macos-x64 --output hello"
  },
  "bin": {
    "hello": "./dist/commands/cli.js"
  },
  "pkg": {
    "scripts": "dist/**/*.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "cli",
    "googlecalendar-api",
    "microsoft-api",
    "notion",
    "notion-api"
  ],
  "author": "Michael Ungar",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ungarmichael/NIS.git"
  },
  "bugs": {
    "url": "git+https://github.com/ungarmichael/NIS/issues"
  },
  "homepage": "https://github.com/ungarmichael/NIS#readme",
  "dependencies": {
    "@googleapis/docs": "^0.2.0",
    "@notionhq/client": "^0.2.3",
    "@types/inquirer": "^8.1.1",
    "axios": "^0.21.1",
    "dotenv": "^10.0.0",
    "esm": "^3.2.25",
    "execa": "^5.1.1",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "googleapis": "^39.2.0",
    "node-fetch": "^2.6.1",
    "pkg-install": "^1.0.0",
    "ts-node-dev": "^1.1.8",
    "yargs": "^17.1.1"
  },
  "devDependencies": {
    "@types/node": "^16.9.0",
    "@types/yargs": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.32.0",
    "nodemon": "^2.0.12",
    "pkg": "^5.3.1",
    "ts-node": "^10.1.0",
    "typecript": "^0.0.1-security",
    "typescript": "^4.4.2"
  }
}
